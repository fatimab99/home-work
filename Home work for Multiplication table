#include <iostream>
using namespace std;

int main()
{
    int i,j;
   cout<<"Multiplication table"<<endl;
   i=1;
   while(i<=10){
        j=1;
    while(j<=10){
        cout<<i<<"*"<<j<<"="<<i*j<<endl;
   j++;
   }
    i++;
   cout<<"\n";
   cout<<"************************"<<endl;
   }

  
    //return 0;
   system("pause");
